{
    "name": "laravelplus/commander",
    "description": "Command execution tracking and management for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LaravelPlus Team",
            "email": "team@laravelplus.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelPlus\\Commander\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelPlus\\Commander\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelPlus\\Commander\\Providers\\CommanderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
